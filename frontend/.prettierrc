{
  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": false,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": true,
  "embeddedLanguageFormatting": "auto",
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "bracketSameLine": true,
  "rangeStart": 0,
  "rangeEnd": null,
  "insertPragma": false,
  "requirePragma": false,
  "disableLanguages": ["vue"],
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "proseWrap": "always",
        "printWidth": 120
      }
    },
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2,
        "singleQuote": false,
        "trailingComma": "none"
      }
    },
    {
      "files": "*.css",
      "options": {
        "singleQuote": false,
        "tabWidth": 2,
        "trailingComma": "none"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "semi": true,
        "singleQuote": false,
        "tabWidth": 2,
        "trailingComma": "es5"
      }
    }
  ]
}